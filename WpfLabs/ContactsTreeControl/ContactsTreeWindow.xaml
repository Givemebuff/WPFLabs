<Window x:Class="WpfLabs.ContactsTreeControl.ContactsTreeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:contactsTreeControl="clr-namespace:WpfLabs.ContactsTreeControl"
        Title="ContactsTreeWindow" Height="300" Width="300">
    <Grid>
        <contactsTreeControl:ContactsTree ItemsSource="{Binding ContactsTreeSource}" SelectedPeople="ContactsTree_OnSelectedPeople" ShowDetail="ContactsTree_OnShowDetail" CallPhone="ContactsTree_OnCallPhone"/>
    </Grid>
</Window>
