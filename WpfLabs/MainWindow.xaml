<Window x:Class="WpfLabs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:wpfLabs="clr-namespace:WpfLabs"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <wpfLabs:ContactsTree ItemsSource="{Binding ContactsTreeSource}" SelectedPeople="ContactsTree_OnSelectedPeople" ShowDetail="ContactsTree_OnShowDetail" CallPhone="ContactsTree_OnCallPhone"/>
    </Grid>
</Window>
